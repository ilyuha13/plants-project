{
  "name": "@plants-project/backend",
  "version": "0.0.0",
  "private": "true",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -b",
    "start": "node ./dist/index.js",
    "pmd": "prisma migrate dev",
    "pgc": "prisma generate",
    "lint": "eslint '**/*.ts' --cache --cache-location ./node_modules/.cache/.eslintcache ",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "prepare": "pnpm pgc"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.14",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@vitest/ui": "^4.0.6",
    "tsx": "^4.20.6",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@plants-project/shared": "workspace:^",
    "@prisma/client": "^6.6.0",
    "@trpc/server": "^11.0.1",
    "@types/lodash": "^4.17.16",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.6.0",
    "sharp": "^0.34.3",
    "superjson": "^2.2.2",
    "trpc-playground": "^1.0.4",
    "zod": "^3.24.2"
  }
}
